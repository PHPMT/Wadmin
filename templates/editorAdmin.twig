{% extends "layouts/layout.twig" %}

{% block title %} Editor Administradores {% endblock %}

{% block content %}

<div class="row vinheta">Publicar Administrador</div>

<div class="row">
	<div class="colun-1">
		<div class="panel-1">
			<div class="title-pag">Publicar Administrador</div>

			<form class="row" action="" method="">

				<div class="row">
					<div class="colun-1 form-group">
						<label>Nome</label>
						<input name="nome" type="text" placeholder="Qual o nome do Administrador?">
					</div>
				</div>

				<div class="row">
					<div class="colun-3 form-group">
						<label>Email</label>
						<input name="nome" type="email" placeholder="Agora o e-mail">
					</div>
					<div class="colun-3 form-group">
						<label>Senha</label>
						<input name="nome" type="password" placeholder="Digite a Senha" >
					</div>
				</div>
				<div class="row">
					<a class="btn-dell right branco" href="mostraAdmin">Voltar</a>
					<input class="btn-add right branco" type="submit" value="Salvar">
				</div>
			</form>
		</div>
	</div>
</div>
{% endblock %}